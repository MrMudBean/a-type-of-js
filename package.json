{
  "name": "a-type-of-js",
  "type": "module",
  "private": true,
  "version": "2.0.0",
  "scripts": {
    "b": "rollup --config rollup.config.js && tsc -p tsconfig.types.json",
    "clean:package": "node scripts/clean-package-json.js",
    "build": "jja rm dist cls && pnpm run b && pnpm run clean:package",
    "build:local": "jja rm dist cls && node scripts/build-local.js",
    "diff": "jja pkg --diff",
    "push:version": "gvv",
    "push": "gvv",
    "eslint": "jja cls && eslint src",
    "beautify": "pnpm run prettier",
    "prettier": "jja cls && prettier . --write",
    "test": "lint-staged && jest --coverage",
    "report": "echo hello || cat ./coverage/lcov.info | coveralls",
    "coverage": "echo hello || codecov",
    "vjj": "vjj",
    "prepublishOnly": "pjj",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,jex,md,mdx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@qqi/check-version": "^1.0.5",
    "@qqi/copy-text": "^1.0.7",
    "@qqi/rollup-external": "^1.0.11",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "a-node-tools": "^4.2.20",
    "color-pen": "^2.0.13",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsdoc": "^61.5.0",
    "globals": "^16.5.0",
    "gvv": "^0.1.8",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-junit": "^16.0.0",
    "jja": "^2.3.21",
    "lint-staged": "^16.2.7",
    "pjj": "^1.0.4",
    "prettier": "^3.7.4",
    "rollup": "^4.54.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "ts-jest": "^29.4.6",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vjj": "^1.0.11"
  }
}